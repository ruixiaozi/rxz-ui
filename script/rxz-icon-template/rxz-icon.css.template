@font-face {
  font-family: "{{fontname}}";
  src: url('../fonts/{{cssPath}}{{fontname}}.eot?t={{timestamp}}'); /* IE9*/
  src: url('../fonts/{{cssPath}}{{fontname}}.eot?t={{timestamp}}#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url("../fonts/{{cssPath}}{{fontname}}.woff2?t={{timestamp}}") format("woff2"),
  url("../fonts/{{cssPath}}{{fontname}}.woff?t={{timestamp}}") format("woff"),
  url('../fonts/{{cssPath}}{{fontname}}.ttf?t={{timestamp}}') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('../fonts/{{cssPath}}{{fontname}}.svg?t={{timestamp}}#{{fontname}}') format('svg'); /* iOS 4.1- */
}

[class^="{{prefix}}-"], [class*=" {{prefix}}-"] {
  font-size:{{fontSize}};
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="{{prefix}}-"]::before, [class*=" {{prefix}}-"]::before  {
  font-family: '{{fontname}}' !important;
}


