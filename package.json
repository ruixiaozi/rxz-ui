{
  "name": "rxz-ui",
  "version": "2.0.0",
  "private": false,
  "main": "lib/rxz-ui.common.js",
  "typings": "lib/packages/index.d.ts",
  "author": "ruixiaozi",
  "email": "admin@ruixiaozi.com",
  "scripts": {
    "serve": "vue-cli-service serve",
    "docs:dev": "npm run gensvg && vuepress-webpack dev docs",
    "docs:build": "npm run gensvg && vuepress-webpack build docs",
    "build": "npm run build:clean && npm run build:lib && ttsc --emitDeclarationOnly",
    "build:clean": "rimraf lib",
    "build:lib": "vue-cli-service build --target lib --name rxz-ui --dest lib packages/index.ts",
    "prebuild": "npm run gensvg && node ./build/prebuild.js",
    "postbuild": "rimraf lib/docs && rimraf lib/example && rimraf lib/tests",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "prepare": "husky install",
    "gen": "node ./script/Generate.js %INIT_CWD%",
    "gensvg": "npm run gensvgjson",
    "gensvgjson": "node ./script/GenerateIcon.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruixiaozi/rxz-ui.git"
  },
  "keywords": [
    "vue",
    "ui",
    "lib",
    "components",
    "utils"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruixiaozi/rxz-ui/issues"
  },
  "homepage": "https://github.com/ruixiaozi/rxz-ui#readme",
  "dependencies": {
    "@tanbo/vue-di-plugin": "^1.0.5",
    "chroma-js": "^2.4.2",
    "core-js": "^3.6.5",
    "lodash": "^4.17.21",
    "rxjs": "^6.5.4",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-property-decorator": "^10.0.0-rc.3"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/chroma-js": "^2.1.4",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-plugin-unit-jest": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "dirlint": "1.0.1",
    "eslint": "^6.7.2",
    "eslint-config-brisk": "1.0.3",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.3",
    "ts-node": "^10.8.1",
    "ttypescript": "^1.5.15",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.6",
    "vue-jest": "^5.0.0-0",
    "vue-router": "^4.1.5",
    "vue-tsc": "^1.2.0",
    "vuepress-webpack": "^2.0.0-beta.49",
    "yalc": "^1.0.0-pre.53"
  }
}
